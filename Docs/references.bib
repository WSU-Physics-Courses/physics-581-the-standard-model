@article{LC:2002,
	author = {Littlejohn, Robert G. and Cargo, Matthew},
	date-modified = {2022-04-19 07:55:30 -0700},
	doi = {10.1063/1.1481388},
	group = {Algorithms; Physics 552},
	journal = {J. Chem. Phys.},
	keywords = {quantum theory; eigenvalues and eigenfunctions; Bessel functions; interpolation; matrix algebra; DVR; algorithms; Hankel transform},
	month = jul,
	number = 1,
	pages = {27-36},
	publisher = {AIP},
	title = {Bessel discrete variable representation bases},
	usedby = {mforbes},
	volume = 117,
	year = 2002,
	abstract = {Discrete variable representation (DVR) basis sets on
the radial half-line, based on Bessel functions, are
presented. These are Hankel transforms of the
eigenfunctions of the particle in a spherical box in
k space, but there is no box or bound on the radial
variable r. The grid points extend to infinity on
the r axis. The DVR functions are exactly
orthonormal and exactly satisfy the interpolation
properties usually associated with DVR
functions. The exact matrix elements of the kinetic
energy are computed, and the use of the Bessel DVR
functions in radial eigenvalue problems is
illustrated. The phase space or semiclassical
interpretation of the Bessel DVR functions is
presented, and variations on these functions,
corresponding to alternative boundary conditions in
k space, are discussed. An interesting feature of
Bessel DVR functions is that they are based on a
finite basis representation that is continuously
infinite.},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://doi.org/10.1063/1.1481388},
	bdsk-url-2 = {http://link.aip.org/link/?JCP/117/27/1}}

